{
	"window.zoomLevel": 2,
	"workbench.colorTheme": "Monokai",
	"files.associations": {
		"*.ect": "html",
		"*.clj": "clojure"
	},
    "workbench.editor.enablePreview": false,
    "breadcrumbs.enabled": false,
    "editor.minimap.enabled": false,
    "scm.autoReveal": false,
    "explorer.autoReveal": false,
    "editor.semanticHighlighting.enabled": true,
	"files.autoSave": "afterDelay",
	"explorer.confirmDelete": false,
	"java.configuration.checkProjectSettingsExclusions": false,
	"editor.suggestSelection": "first",
	"vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
	"explorer.confirmDragAndDrop": false,
	"editor.detectIndentation": true,
	"editor.tokenColorCustomizations": {
		"textMateRules": [
			{
				"scope": [
					"constant.keyword.clojure"
				],
				"settings": {
					"foreground": "#7afcf5"
				}
			},
			{
				"scope": [
					"punctuation.section.expression.begin.clojure",
					"punctuation.section.expression.end.trailing.clojure"
				],
				"settings": {
					"foreground": "#999"
				}
			}
		]
	},
	"calva.paredit.defaultKeyMap": "none",
	"calva.lintOnSave": false,
	"bracket-pair-colorizer-2.colors": [
		"LightGray",
		"Orchid",
		"Gold",
		"LightSkyBlue",
		"LightGreen"
	],
	"calva.testOnSave": true,
	"calva.evalOnSave": true,
	"workbench.startupEditor": "newUntitledFile",
	"debug.chrome.useV3": true,
	"dart.previewLsp": true,
	"dart.previewFlutterUiGuides": true,
	"[dart]": {
		"editor.formatOnSave": true,
		"editor.formatOnType": true,
		"editor.rulers": [
			80
		],
		"editor.selectionHighlight": false,
		"editor.suggest.snippetsPreventQuickSuggestions": false,
		"editor.suggestSelection": "first",
		"editor.tabCompletion": "onlySnippets",
		"editor.wordBasedSuggestions": false,
		"files.insertFinalNewline": true
	},
	"debug.onTaskErrors": "debugAnyway",
	"java.semanticHighlighting.enabled": true,
	"html.trace.server": "verbose",
	"dart.flutterSdkPath": "/home/greg/flutter",
	"calva.referencesCodeLens.enabled": true,
	"clojure.trace.server": "verbose",
	"calva.clojureLspPath": "/home/greg/dev/clojure-lsp/clojure-lsp",
	"editor.semanticHighlighting.enabled": true
}
